import java.util.Scanner;

public class Main{
  public static void main(String[] args){
Scanner sc = new Scanner(System.in);
int n1=sc.nextInt();
int n2=sc.nextInt();
int a=0;
int b=0;
int price=0;
int result=0;

for(int i=0; i<n2; i++){
a=sc.nextInt();
b=sc.nextInt();
price=a*b;
result+=price;
}
if(result==n1){
  System.out.println("Yes");
}else{
  System.out.println("No");
}
  }
}