package baekjoon;

import java.util.Scanner;

public class Baekjoon1330{
public static void main(String[] args){
  Scanner sc = new Scanner(System.in);
int a = sc.nextInt();
int b = sc.nextInt();
if(-10000<=a&&b<=10000){
if(a>b){
System.out.println(">");
}else if(a<b){
System.out.println("<");
}else{
System.out.println("==");
}
}
}
 }